<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.x.mapper.CommentMapper">
    <select id="com" parameterType="int" resultType="Comment">
        select *
        from housedb.t_comment
        where item_id = #{id}
    </select>
</mapper>
